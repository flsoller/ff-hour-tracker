{
  "compilerOptions": {
    "moduleResolution": "NodeNext",
    "lib": [
      "ES2022"
    ],
    "sourceMap": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "target": "es2022",
    "allowJs": true,
    "resolveJsonModule": true,
    "moduleDetection": "force",
    "isolatedModules": true,
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "module": "NodeNext",
    "declaration": true,
    "declarationMap": true,
    "forceConsistentCasingInFileNames": true
  }
}