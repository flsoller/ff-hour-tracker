{
  "name": "api",
  "version": "0.0.1",
  "private": true,
  "description": "API for hour tracker app",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "build": "rimraf dist && tsc",
    "db:docker:firstinit": "cd .. && docker-compose -f docker-compose.dev.yml exec api npx prisma db push --force-reset",
    "db:console": "cd .. && docker-compose -f docker-compose.dev.yml exec postgres psql -U postgres -d app_db_dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flsoller/ff-hour-tracker.git"
  },
  "author": "Florian Soller",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/flsoller/ff-hour-tracker/issues"
  },
  "homepage": "https://github.com/flsoller/ff-hour-tracker#readme",
  "dependencies": {
    "@prisma/client": "^3.2.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4"
  },
  "devDependencies": {
    "@tsconfig/node14": "^1.0.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.0",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "nodemon": "^2.0.13",
    "prettier": "^2.4.1",
    "prisma": "^3.2.1",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.4"
  }
}
