{
  "name": "api",
  "version": "0.0.1",
  "private": true,
  "description": "API for hour tracker app",
  "main": "server.js",
  "scripts": {
    "test": "dotenv -e .env.test jest -i",
    "start": "node dist/server.js",
    "dev": "dotenv -e .env.dev nodemon src/server.ts",
    "build": "rimraf dist && tsc",
    "db:docker:firstinit": "cd .. && docker-compose -f docker-compose.dev.yml exec api yarn prisma db push --force-reset",
    "db:console": "cd .. && docker-compose -f docker-compose.dev.yml exec postgres psql -U postgres -d app_db_dev",
    "test-ci:integration": "cd .. && docker-compose -f docker-compose.dev.yml exec api yarn test",
    "prisma-generate": "npx prisma generate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flsoller/ff-hour-tracker.git"
  },
  "author": "Florian Soller",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/flsoller/ff-hour-tracker/issues"
  },
  "homepage": "https://github.com/flsoller/ff-hour-tracker#readme",
  "dependencies": {
    "@prisma/client": "^3.12.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "dotenv-cli": "^5.1.0",
    "express": "^4.17.3",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.13",
    "prettier": "^2.4.1",
    "prisma": "^3.12.0",
    "supertest": "^6.2.3",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
