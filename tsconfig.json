{
  "compilerOptions": {
    "moduleResolution": "node16",
    "lib": [
      "ES2022"
    ],
    "sourceMap": true,
    "target": "ES2022",
    "module": "Node16",
    "allowJs": true,
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "noEmit": true,
    "declaration": true,
    "declarationMap": true,
    "noLib": false
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
